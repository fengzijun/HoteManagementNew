


@model HoteManagement.Web.Models.Api.CreateProjectSettlementRequest
@{
    ViewBag.Title = "CreateProjectSettlement";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
    var userid = ViewBag.UserId == null ? UserInfo.LoginID : ViewBag.UserId;
}

<div class="right_col" role="main">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">新增竣工结算</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        @using (Html.BeginForm("CreateProjectSettlement", "Admin", FormMethod.Post, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
        {

            <div class="panel panel-primary">
                <div class="panel-heading">工程基本信息</div>
                <div class="panel-body">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">项目名称</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.projectname, new { @class = "form-control" })
                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">工程地址</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.projectaddress, new { @class = "form-control" })
                        </div>
                    </div>

                    <div class="form-group">


                        <label for="inputEmail3" class="col-sm-1 control-label">规模（m2或m）</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.projectsope, new { @class = "form-control" })
                        </div>
                        <label for="inputEmail3" class="col-sm-1 control-label">经办单位类型</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @*<input type="file" name="contract_file" />*@
                            @Html.TextBoxFor(s => s.unittype, new { @class = "form-control" })

                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">结算审核受理时间</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.auditstarttime, new { @class = "form-control datetime" })

                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">结算审定时间</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.auditendtime, new { @class = "form-control datetime" })

                        </div>
                    </div>


                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">审核超期原因</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.timeoutreasion, new { @class = "form-control" })

                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">中标价（万元）</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.zprice, new { @class = "form-control" })

                        </div>
                    </div>



                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">合同价（万元）</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.cprice, new { @class = "form-control" })

                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">送审价（万元）</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.sprice, new { @class = "form-control" })

                        </div>
                    </div>


                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">审定价（万元）</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.sdprice, new { @class = "form-control" })

                        </div>


                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">工程特征</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @*@Html.DropDownListFor(s => s.projecttype, ViewBag.ProjectList as List<SelectListItem>, new { @class = "form-control" })*@
                            @Html.TextAreaFor(s => s.projectfeature, new { @class = "form-control" })
                        </div>

                    </div>




                </div>
            </div>

            <div class="panel panel-primary">
                <div class="panel-heading">建设单位信息</div>
                <div class="panel-body">
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">单位名称</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.unitname, new { @class = "form-control" })

                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">联系人</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.contact, new { @class = "form-control" })

                        </div>
                    </div>

                    <div class="form-group">


                        <label for="inputEmail3" class="col-sm-1 control-label">手机号码</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.mobile, new { @class = "form-control" })

                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">单位号码</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.unitphone, new { @class = "form-control" })

                        </div>
                    </div>
                </div>
            </div>

            <div class="panel panel-primary">
                <div class="panel-heading">造价咨询单位信息</div>
                <div class="panel-body">
                    <div class="form-group">

                        <label for="inputEmail3" class="col-sm-1 control-label">单位名称</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.unitname2, new { @class = "form-control" })

                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">资质等级</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.level, new { @class = "form-control" })

                        </div>
                    </div>


                    <div class="form-group">


                        <label for="inputEmail3" class="col-sm-1 control-label">结算审核人</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.contact2, new { @class = "form-control" })

                        </div>

                        <label for="inputEmail3" class="col-sm-1 control-label">联系方式</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.mobile2, new { @class = "form-control" })

                        </div>
                    </div>


                    <div class="form-group">

                        <label for="inputEmail3" class="col-sm-1 control-label">执业资格</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.qualification, new { @class = "form-control" })

                        </div>
                        <label for="inputEmail3" class="col-sm-1 control-label">证书编号</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.certificate, new { @class = "form-control" })

                        </div>

                    </div>
                </div>
            </div>

            <div class="panel panel-primary">
                <div class="panel-heading">施工单位信息</div>
                <div class="panel-body">

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-1 control-label">单位名称</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.unitname3, new { @class = "form-control" })

                        </div>
                        <label for="inputEmail3" class="col-sm-1 control-label">资质等级</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.orgcode3, new { @class = "form-control" })

                        </div>

                    </div>



                    <div class="form-group">


                        <label for="inputEmail3" class="col-sm-1 control-label">结算编制人</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.contact3, new { @class = "form-control" })

                        </div>
                        <label for="inputEmail3" class="col-sm-1 control-label">联系方式</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.mobile3, new { @class = "form-control" })

                        </div>
                    </div>

                    <div class="form-group">

                        <label for="inputEmail3" class="col-sm-1 control-label">执业资格</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.qualification2, new { @class = "form-control" })

                        </div>
                        <label for="inputEmail3" class="col-sm-1 control-label">证书编号</label>
                        <div class="col-sm-5">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            @Html.TextBoxFor(s => s.certificate2, new { @class = "form-control" })

                        </div>

                    </div>

                </div>
            </div>

            <div class="panel panel-primary">
                <div class="panel-heading">添加附件</div>
                <div class="panel-body">

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">苏州市吴江区建设工程竣工结算备案申请表</label>
                        <div class="col-sm-10">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            <input type="file" name="applyfile" />

                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">苏州市吴江区建设工程竣工结算备案表</label>
                        <div class="col-sm-10">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            <input type="file" name="recordfile" />

                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">工程造价咨询报告书</label>
                        <div class="col-sm-10">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            <input type="file" name="reportfile" />

                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">工程结算审定单</label>
                        <div class="col-sm-10">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            <input type="file" name="pricefile" />

                        </div>
                    </div>

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label">其他</label>
                        <div class="col-sm-10">
                            @*<input type="email" class="form-control" id="inputEmail3" placeholder="Email">*@
                            <input type="file" name="otherfile" />

                        </div>
                    </div>

                </div>
            </div>


            @Html.HiddenFor(s => s.UserId);

            <div class="form-group">
                <label for="inputEmail3" class="col-sm-1 control-label"></label>
                <div class="col-sm-5">
                    @Html.Partial("DisplayErrorView")
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-offset-1 col-sm-8">

                    <input value="提交" type="submit" class="btn btn-primary" />
                    @if (userid != 0)
                    {
                        <a value="提交" class="btn btn-primary" href="/admin/GetProjectSettlement?loginid=@userid">返回</a>
                    }
                    else
                    {
                        <a value="提交" class="btn btn-primary" href="/admin/GetProjectSettlement">返回</a>
                    }

                </div>
            </div>
        }
    </div>

</div>

