<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>后台管理系统</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/content/bootstrap/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- MetisMenu CSS -->
    <link href="~/content/bootstrap/vendor/metisMenu/metisMenu.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="~/content/bootstrap/dist/css/sb-admin-2.css" rel="stylesheet">

    <!-- Morris Charts CSS -->
    <link href="~/content/bootstrap/vendor/morrisjs/morris.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="~/content/bootstrap/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">

    <link href="~/content/bootstrap-datetimepicker.min.css" rel="stylesheet" type="text/css">

    
   

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <div id="wrapper">

        <!-- Navigation -->
        @Html.Partial("NavLayout")

        @RenderBody()

        @Html.Partial("LoaddingLayout")
        @Html.Partial("LoadingResult")
    </div>
    <!-- /#wrapper -->
    @*<script src="~/content/bootstrap/vendor/jquery/jquery.min.js"></script>*@
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="~/content/bootstrap/vendor/bootstrap/js/bootstrap.min.js"></script>

    <!-- Metis Menu Plugin JavaScript -->
    <script src="~/content/bootstrap/vendor/metisMenu/metisMenu.min.js"></script>

    <!-- Morris Charts JavaScript -->
    @*<script src="~/content/bootstrap/vendor/raphael/raphael.min.js"></script>
        <script src="~/content/bootstrap/vendor/morrisjs/morris.min.js"></script>
        <script src="~/content/bootstrap/data/morris-data.js"></script>*@

    <!-- Custom Theme JavaScript -->
    <script src="~/content/bootstrap/dist/js/sb-admin-2.js"></script>
    <script src="~/content/js/moment-with-locales.min.js"></script>
    <script src="~/content/js/bootstrap-datetimepicker.min.js"></script>




    <script>
        $(function () {

            $('.datetime').datetimepicker({
                format: 'YYYY-MM-DD',
                locale: moment.locale('zh-cn')
            });

            function showloadding() {
                $('#loadview').modal('show');
            }

            function hiddenloadding() {
                $('#loadview').modal('hide');
            }

        })

    </script>

    @RenderSection("scripts", required: false)


</body>

</html>
